@prefix : <https://complex.example.com/laderr#> .
@prefix laderr: <https://w3id.org/pedropaulofb/laderr#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:LaderrSpecification a laderr:LaderrSpecification ;
    laderr:baseUri "https://complex.example.com/laderr#"^^xsd:anyURI ;
    laderr:composedOf :c1,
        :c2,
        :c3,
        :o1,
        :o2,
        :o3,
        :v1 ;
    laderr:createdBy "Alice"^^xsd:string,
        "Bob"^^xsd:string,
        "Charlie"^^xsd:string ;
    laderr:createdOn "2025-01-17T12:00:01+00:00"^^xsd:dateTime ;
    laderr:description "A complex specification for testing all LaDeRR rules with SHACL compliance."^^xsd:string ;
    laderr:modifiedOn "2025-01-18T12:00:02+00:00"^^xsd:dateTime ;
    laderr:title "Complex LaDeRR Specification"^^xsd:string ;
    laderr:version "2.0"^^xsd:string .

:o2 a laderr:Object ;
    rdfs:label "Secondary Object" ;
    laderr:capabilities :c2 ;
    laderr:inhibits :o1 ;
    laderr:protects :o1 .

:o3 a laderr:Object ;
    rdfs:label "Tertiary Object" ;
    laderr:capabilities :c3 ;
    laderr:threatens :o1 .

:RAD a laderr:Resilience ;
    laderr:preserves :c1 ;
    laderr:preservesAgainst :c3 ;
    laderr:preservesDespite :v1 .

:c2 a laderr:Capability ;
    rdfs:label "Capability 2" ;
    laderr:description "Disables vulnerabilities." ;
    laderr:disables :c1,
        :v1 ;
    laderr:state laderr:enabled ;
    laderr:sustains :RAD .

:c3 a laderr:Capability ;
    rdfs:label "Capability 3" ;
    laderr:description "Exploits vulnerabilities." ;
    laderr:exploits :v1 ;
    laderr:state laderr:enabled .

:o1 a laderr:Object ;
    rdfs:label "Primary Object" ;
    laderr:capabilities :c1 ;
    laderr:resiliences :RAD ;
    laderr:vulnerabilities :v1 .

:c1 a laderr:Capability ;
    rdfs:label "Capability 1" ;
    laderr:description "Provides core functionality." ;
    laderr:state laderr:enabled .

:v1 a laderr:Vulnerability ;
    rdfs:label "Vulnerability 1" ;
    laderr:description "A vulnerability exploitable by c3 and mitigated by c2." ;
    laderr:exposes :c1 ;
    laderr:state laderr:enabled .

