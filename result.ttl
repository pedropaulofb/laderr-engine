laderr:RiskEntity a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty laderr:name ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty laderr:description ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty laderr:id ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

laderr:baseURI a owl:DatatypeProperty ;
    rdfs:range xsd:anyURI .

laderr:composedOf a owl:ObjectProperty .

laderr:createdBy a owl:DatatypeProperty ;
    rdfs:range xsd:string .

laderr:createdOn a owl:DatatypeProperty ;
    rdfs:range xsd:dateTime .

laderr:id a owl:DatatypeProperty ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

laderr:modifiedOn a owl:DatatypeProperty ;
    rdfs:range xsd:dateTime .

laderr:name a owl:DatatypeProperty ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

laderr:title a owl:DatatypeProperty ;
    rdfs:range xsd:string .

laderr:version a owl:DatatypeProperty ;
    rdfs:range xsd:string .

